  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "Linear Regression model to predict Patch Times(Days)."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Mean Absolute Error: 31.960535042332364\n",
      "Root Mean Squared Error: 35.74237003665914\n"
     ]
    }
   ],
   "source": [
    "model = LinearRegression()\n",
    "model.fit(X_train, y_train)\n",
    "y_pred = model.predict(X_test)\n",
    "mae = mean_absolute_error(y_test, y_pred)\n",
    "rmse = np.sqrt(mean_squared_error(y_test, y_pred))\n",
    "print(\"Mean Absolute Error:\", mae)\n",
    "print(\"Root Mean Squared Error:\", rmse)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 27,
   "metadata": {},
   "outputs": [],
   "source": [
    "#Applying Standard Scaler and Ridge Regression to predict Patch Times\n",
    "pipeline = Pipeline([\n",
    "    ('scaler', StandardScaler()),\n",
    "    ('ridge', Ridge())\n",
    "])\n",
    "param_grid = {\n",
    "    'ridge__alpha': [0.1, 1.0, 10.0, 100.0],\n",
    "    'ridge__solver': ['auto', 'svd', 'cholesky', 'lsqr', 'sag']\n",
    "}"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 40,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Best Parameters: {'ridge__alpha': 10.0, 'ridge__solver': 'sag'}\n",
      "Mean Absolute Error (Tuned Ridge Regression): 31.050073313569083\n",
      "Root Mean Squared Error (Tuned Ridge Regression): 34.811687501276154\n"
     ]
    }
   ],
   "source": [
    "grid_search = GridSearchCV(pipeline, param_grid, cv=5, scoring='neg_mean_squared_error')\n",
    "grid_search.fit(X_train, y_train)\n",
    "best_model = grid_search.best_estimator_\n",
    "y_pred_best = best_model.predict(X_test)\n",
    "mae_best = mean_absolute_error(y_test, y_pred_best)\n",
    "rmse_best = np.sqrt(mean_squared_error(y_test, y_pred_best))\n",
    "print(\"Best Parameters:\", grid_search.best_params_)\n",
    "print(\"Mean Absolute Error (Tuned Ridge Regression):\", mae_best)\n",
    "print(\"Root Mean Squared Error (Tuned Ridge Regression):\", rmse_best)"
   ]
  },
